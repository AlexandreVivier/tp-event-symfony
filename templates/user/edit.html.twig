{% extends 'base.html.twig' %}
{% block title %}
Modifier votre profil
{% endblock %}
{% block body %}

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h1>Modifier votre profil</h1>
            <hr>
            {{ form_start(form) }}
                {{ form_row(form.name) }}
                {{ form_row(form.email) }}
                {{ form_row(form.plainPassword) }}
                {{ form_row(form.submit) }}
            {{ form_end(form) }}
        </div>
    </div>
</div>
{% endblock %}